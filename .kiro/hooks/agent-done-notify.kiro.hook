{
  "enabled": true,
  "name": "Уведомление о завершении",
  "description": "Воспроизводит звук и показывает нативное Windows уведомление когда агент завершил работу",
  "version": "1",
  "when": {
    "type": "agentStop"
  },
  "then": {
    "type": "runCommand",
    "command": "powershell -Command \"[System.Media.SystemSounds]::Asterisk.Play(); "
  },
  "workspaceFolderName": "TavryaSchedule",
  "shortName": "agent-done-notify"
}